{
  "name": "vite-vue3-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prepare": "husky install",
  "dependencies": {
    "axios": "^1.7.9",
    "element-plus": "^2.9.4",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-standard": "^9.0.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.2",
    "postcss-preset-env": "^10.1.4",
    "prettier": "^3.5.2",
    "sass": "^1.85.0",
    "typescript": "~5.7.2",
    "unplugin-auto-import": "^19.1.0",
    "unplugin-element-plus": "^0.9.1",
    "unplugin-vue-components": "^28.4.0",
    "vite": "^6.1.0",
    "vue-tsc": "^2.2.0"
  }
